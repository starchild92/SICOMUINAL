							<div class="panel-body">
								<h4 class="bottom-line">{{object.quienes|length}} Respondieron "{{ object.nivel }}"</h4>
								{% for jgf in object.quienes %}
								<a class="label label-primary" target="_blank" href="{{ path('jefegrupofamiliar_show', {'id' : jgf.id }) }}">{{ jgf.nombreyapellido() }} <i class="fa fa-external-link"></i></a>
								{% endfor %}
							</div>
						{% endfor %}
					</div>
				</div>
				<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
					<div class="panel panel-success">
						<p class="panel-title text-center negritas">Niveles de Instrucción - Personas de la Comunidad</p>
						{% for object in personas.stat_instruccion_p %}
							<div class="panel-body">
								<h4 class="bottom-line">{{object.quienes|length}} Respondieron "{{ object.nivel }}"</h4>
								{% for p in object.quienes %}
								<a class="label label-success" target="_blank" href="{{ path('personas_show', {'id' : p.id }) }}">{{ p.nombreyapellido() }} <i class="fa fa-external-link"></i></a>
								{% endfor %}
							</div>
						{% endfor %}
					</div>
				</div>


