{% extends 'base.html.twig' %}

{% block body %}
    <h1>AdminIncapacidades list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Incapacidad</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for adminIncapacidade in adminIncapacidades %}
            <tr>
                <td><a href="{{ path('configurable_incapacidades_show', { 'id': adminIncapacidade.id }) }}">{{ adminIncapacidade.id }}</a></td>
                <td>{{ adminIncapacidade.incapacidad }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('configurable_incapacidades_show', { 'id': adminIncapacidade.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('configurable_incapacidades_edit', { 'id': adminIncapacidade.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('configurable_incapacidades_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
