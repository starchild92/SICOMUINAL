        <div class="panel-heading">Servicios Comunales Disponibles</div>
        <table class="table-striped table table-bordered">
            <thead>
                <tr>
                    <th>Nombre</th>
                    <th>Opciones</th>
                </tr>
            </thead>
            <tbody>
            {% for adminServiciosComunale in adminServiciosComunales %}
                <tr>
                    <td>{{ adminServiciosComunale.nombre }}</td>
                    <td>
                        <a id="forModalEdit" href="{{ path('configurables_tipos_servicios_comunales_edit', { 'id': adminServiciosComunale.id }) }}"><i class="fa fa-edit"></i> Editar</a>
                    </td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>        
    <a class="btn btn-primary" href="{{ path('configurables_tipos_servicios_comunales_new') }}" id="forModal"><i class="fa fa-plus"></i> Agregar Nuevo Mecanismo de Informaci√≥n</a>
{% endblock %}