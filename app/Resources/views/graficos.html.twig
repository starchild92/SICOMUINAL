{% extends 'base.html.twig' %}
{% block javascripts %}
    <script type="text/javascript" src="{{ asset('js/packery.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/highchart.js') }}"></script>
    <script type="text/javascript" src="{{ asset('js/highchart/highcharts.js') }}"></script>
    <script type="text/javascript" src="{{ asset('datatables/datatables.min.js') }}"></script>
    <script>
        $(document).ready(function() {
            $('table.highchart').highchartTable();
            $('.row.grid').packery({
              itemSelector: 'div[class*="col-"',
            });
            dataTable = $('table.table').DataTable({
                "aaSorting": [[ 1, "desc" ]],
                "language": {
                    "url": "{{ asset('datatables/espanol.js') }}"
                },
                "dom": "t",
                "paging": false,
            });
        });
    </script>
{% endblock %}