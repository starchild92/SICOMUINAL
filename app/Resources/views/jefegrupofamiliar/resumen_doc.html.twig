<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <link rel="stylesheet" type="text/css" href="{{ base_dir }}{{ asset('css/bootstrap.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ base_dir }}{{ asset('css/estilos.css') }}">
        <link rel="stylesheet" type="text/css" href="{{ base_dir }}{{ asset('css/font-awesome.css') }}">
</head>
<body>
    <h2>Estadísticas Jefes de Grupos Familiares</h2>
    <hr>
    <div class="row">
        <div class="col-sm-3">
            <div class="panel panel-primary">
                <div class="panel-heading">Nacionalidad</div>
                <table class="table table-bordered table-striped table-hover table-condensed highchart" data-graph-container-before="1" data-graph-type="pie" data-graph-height="300">
                    <thead>
                        <tr>
                            <th>Nacionalidad</th>
                            <th>Cant.</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for n in stat_nacionalidad %}
                        <tr>
                            <td>{{n.nacionalidad}}</td>
                            <td>{{n.cantidad}}</td>
                        </tr>
                        {% endfor %}
                    </tbody>
                </table>
            </div>
        </div>

        <div class="col-sm-3">
            <div class="panel panel-primary">
                <div class="panel-heading">Sexo</div>
                <table class="table table-bordered table-striped table-hover table-condensed highchart" data-graph-container-before="1" data-graph-type="pie" data-graph-height="300">
                    <thead>
                        <tr>
                            <th>Sexo</th>
                            <th>Cant.</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for sx in stat_sexo %}
                        <tr>
                            <td>{{sx.sexo}}</td>
                            <td>{{sx.cantidad}}</td>
                        </tr>{% endfor %}
                    </tbody>
                </table>
            </div>
        </div>

        <div class="col-sm-3">
            <div class="panel panel-primary">
                <div class="panel-heading">Inscrito en el CNE</div>
                <table class="table table-bordered table-striped table-hover table-condensed highchart" data-graph-container-before="1" data-graph-type="pie" data-graph-height="300">
                    <thead>
                        <tr>
                            <th>Resp</th>
                            <th>Cant.</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for cne in stat_cne %}
                        <tr>
                            <td>{{cne.resp}}</td>
                            <td>{{cne.cantidad}}</td>
                        </tr>{% endfor %}
                    </tbody>
                </table>
            </div>
        </div>

        <div class="col-sm-3">
            <div class="panel panel-primary">
                <div class="panel-heading">Edo. Civil</div>
                <table class="table table-bordered table-striped table-hover table-condensed highchart" data-graph-container-before="1" data-graph-type="pie" data-graph-height="300">
                    <thead>
                        <tr>
                            <th>Estado Civil</th>
                            <th>Cant.</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for edo in stat_edo_civil %}
                        <tr>
                            <td>{{edo.edo_civil}}</td>
                            <td>{{edo.cantidad}}</td>
                        </tr>{% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
    {# dlfjkhslkdhflkshdlfkjhslkdjfhslkjdfhlskjdfh #}
