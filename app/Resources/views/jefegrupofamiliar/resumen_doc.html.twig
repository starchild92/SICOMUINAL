                    <thead>
                        <tr>
                            <th>Nivel</th>
                            <th>Cant.</th>
                        </tr>
                    </thead>
                    <tbody>
                        {% for nivel in stat_instruccion %}<tr>
                            <td>{{ nivel.instruccion }}</td>
                            <td>{{ nivel.cantidad }}</td>
                        </tr>{% endfor %}
                    </tbody>
                </table>
            </div>
        </div>
        <div class="col-sm-3">
            <div class="panel panel-primary">
                <div class="panel-heading">Profesiones</div>
                <table class="table table-bordered table-striped table-hover table-condensed highchart" data-graph-container-before="1" data-graph-type="pie" data-graph-height="300">
